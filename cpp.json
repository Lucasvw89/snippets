    "bin_search": {
        "prefix": "bins",
        "body": [
            "// array is ${1:arr} || target is ${2:target}",
            "int bl = 0, br = $1.size() - 1;",
            "int bmid;",
            "while (bl <= br) {",
            "    bmid = bl + ((br - bl) >> 1);",
            "    if ($2 == $1[bmid]) { break; }",
            "    else if ($2 < $1[bmid]) br = bmid - 1;",
            "    else bl = bmid + 1;",
            "}"
        ],
        "description": "binary search"
    },
